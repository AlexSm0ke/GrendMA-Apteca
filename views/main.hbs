<div class="main">
{{#each products}}
  <div class="oneItem">
    <div class="name">{{this.product_name}}</div>
    <div class="divImg">
      <img
        class="img"
        src={{this.picture}}
        alt=""
      />
    </div>
    <div class="price">{{this.price}} ₽.</div>
    <a href="#" id="{{id}}" name ="toBusket" class="btn btn-primary";>В корзину</a>
  </div>
{{/each}}
</div>




